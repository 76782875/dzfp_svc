<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rjxx.taxeasy.dao.FphxwsjlSbMapper">

    <select id="findOneByParams" parameterType="map" resultType="fphxwsjlSb">
        select * from t_fphxwsjl_sb where 1=1
        <if test="kplsh != null">
            and kplsh = #{kplsh}
        </if>
        <if test="gsdm != null and gsdm !=''">
            and gsdm = #{gsdm}
        </if>
        <if test="orderBy != null">
            order by ${orderBy}
        </if>
        limit 1
    </select>

    <select id="findAllByParams" parameterType="map" resultType="fphxwsjlSb">
        select * from t_fphxwsjl_sb where yxbz = '1'
        <if test="orderBy != null">
            order by ${orderBy}
        </if>
    </select>

    <select id="findByPage" parameterType="pagination" resultType="fphxwsjlSb">
        select * from t_fphxwsjl_sb where yxbz = '1'
        <if test="params.orderBy != null">
            order by ${params.orderBy}
        </if>
    </select>

</mapper>

